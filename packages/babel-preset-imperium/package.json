{
	"name": "@imperium/babel-preset-imperium",
	"version": "2.0.0",
	"description": "Babel preset used by Imperium.",
	"repository": "darkadept/imperium",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/darkadept/imperium/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"index.js",
		"create.js",
		"dependencies.js",
		"dev.js",
		"prod.js",
		"test.js"
	],
	"scripts": {
		"lint": "eslint --cache '*.js'",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "../../tools/coverage.sh babel-preset-imperium '*.js'",
		"docs:md": "../../tools/jsdocmd.sh babel-preset-imperium",
		"yalc:publish": "yalc publish --push"
	},
	"dependencies": {
		"@babel/core": "^7.4.0",
		"@babel/plugin-proposal-class-properties": "^7.4.0",
		"@babel/plugin-proposal-object-rest-spread": "^7.4.0",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/plugin-syntax-object-rest-spread": "^7.2.0",
		"@babel/plugin-transform-classes": "^7.4.0",
		"@babel/plugin-transform-destructuring": "^7.4.0",
		"@babel/plugin-transform-react-constant-elements": "^7.2.0",
		"@babel/plugin-transform-react-display-name": "^7.2.0",
		"@babel/plugin-transform-regenerator": "^7.4.0",
		"@babel/plugin-transform-runtime": "^7.4.0",
		"@babel/preset-env": "^7.4.1",
		"@babel/preset-flow": "^7.0.0",
		"@babel/preset-react": "^7.0.0",
		"@babel/preset-typescript": "^7.3.3",
		"babel-plugin-flow-react-proptypes": "^25.0.0",
		"babel-plugin-inline-import": "^3.0.0",
		"babel-plugin-macros": "^2.5.0",
		"babel-plugin-transform-dynamic-import": "^2.1.0",
		"babel-plugin-transform-react-remove-prop-types": "^0.4.24"
	},
	"jest": {
		"transform": {
			"^.+\\.[tj]sx?$": "<rootDir>/../../tools/jest-transformer.js"
		},
		"testPathIgnorePatterns": [
			"/node_modules/",
			"<rootDir>/test.js"
		]
	},
	"gitHead": "ef0f65a07cdee2c41888f59f00239a4961884a42"
}
