{
	"name": "@imperium/layout",
	"version": "7.0.1",
	"description": "Imperium Layout package",
	"bugs": {
		"url": "https://github.com/darkadept/imperium/issues"
	},
	"repository": "darkadept/imperium",
	"license": "MIT",
	"author": "Mike Kornelson <darkadept@durbn.net>",
	"main": "client.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"client.js"
	],
	"scripts": {
		"build": "yarn build:dev && yarn build:prod",
		"build:dev": "NODE_ENV=development webpack --config webpack.js",
		"build:prod": "NODE_ENV=production webpack --config webpack.js",
		"clean": "rimraf ./dist && rimraf .eslintcache",
		"depcheck": "depcheck",
		"lint": "eslint --cache --ext js,ts,tsx src",
		"lint:fix": "eslint --cache --fix --ext js,ts,tsx src",
		"sort": "npx sort-package-json",
		"start": "screen -c ./dev.screenrc",
		"ts": "tsc"
	},
	"eslintConfig": {
		"parserOptions": {
			"project": "./tsconfig-eslint.json"
		}
	},
	"dependencies": {
		"@imperium/client": "^7.0.0",
		"@imperium/state": "^7.0.0",
		"debug": "^4.1.1",
		"lodash": "^4.17.21",
		"mingo": "^4.1.4",
		"query-string": "^7.0.1",
		"react-responsive": "^8.2.0",
		"semantic-ui-react": "^2.0.3"
	},
	"peerDependencies": {
		"@reduxjs/toolkit": "^1.x",
		"react": "^17.x",
		"react-redux": "^7.x",
		"react-router-dom": "5.x"
	},
	"engines": {
		"node": ">=14"
	},
	"publishConfig": {
		"access": "public"
	}
}
