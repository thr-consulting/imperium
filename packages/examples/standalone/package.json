{
  "name": "@imperium/example-standalone",
  "description": "Imperium standalone example project",
  "version": "4.3.0",
  "author": "Mike Kornelson <darkadept@durbn.net>",
  "repository": {
    "type": "git",
    "url": "git@github.com:darkadept/imperium.git"
  },
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=12"
  },
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development node index.js",
    "start:prod": "NODE_ENV=production node index.js",
    "build": "NODE_ENV=production babel src --extensions \".ts\" --out-dir dist --source-maps",
    "clean": "rimraf ./dist && rimraf .eslintcache",
    "Xlint": "eslint --cache --ext ts,js src",
    "Xlint:fix": "eslint --cache --fix --ext ts,js src",
    "ts": "tsc"
  },
  "dependencies": {
    "@imperium/connector": "^4.3.0",
    "@imperium/example-domain": "^4.3.0",
    "@thx/sharedcache": "^11.0.2",
    "debug": "^4.1.1",
    "dotenv": "^8.1.0",
    "lodash": "^4.17.15",
    "pg": "^8.2.0",
    "redis": "^3.0.2",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@imperium/babel-preset-imperium": "^4.3.0",
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.149"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikroorm.ts"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "project": "../../../tsconfig.json"
    }
  }
}
