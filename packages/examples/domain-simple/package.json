{
	"name": "@imperium/example-domain-simple",
	"version": "4.1.0",
	"description": "Imperium Simple Domain Example",
	"author": "Mike Kornelson <darkadept@durbn.net>",
	"private": true,
	"engines": {
		"node": ">=12"
	},
	"main": "index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"index.js"
	],
	"scripts": {
		"start": "screen -c ./domain.screenrc",
		"build": "yarn build:dev && yarn build:prod",
		"build:dev": "NODE_ENV=development webpack --config webpack.js",
		"build:prod": "NODE_ENV=production webpack --config webpack.js",
		"clean": "rimraf ./dist",
		"lint": "eslint --ext js,ts src",
		"lint:fix": "eslint --cache --fix --ext js,ts src",
		"ts": "tsc",
		"yalc:publish": "yalc publish --push",
		"prettier": "prettier"
	},
	"dependencies": {
		"@imperium/util": "^4.1.0",
		"debug": "^4.1.1"
	}
}
